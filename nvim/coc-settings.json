{
    "jedi.enable": true,
    "jedi.executable.command": "jedi-language-server",
    "jedi.hover.enable": false,
    "languageserver": {
        "terraform": {
            "command": "terraform-ls",
            "args": ["serve"],
            "filetypes": [
                "terraform",
                "tf"
            ],
            "initializationOptions": {},
            "settings": {}
        },
        "go": {
          "command": "gopls",
          "rootPatterns": ["go.mod"],
          "trace.server": "verbose",
          "filetypes": ["go"]
        }
        
    }
}
